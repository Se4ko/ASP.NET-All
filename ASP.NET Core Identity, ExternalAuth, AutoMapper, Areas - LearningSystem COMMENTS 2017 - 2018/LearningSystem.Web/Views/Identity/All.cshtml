@model IEnumerable<ListUserViewModel>

@{
    ViewData["Title"] = "All Users";
}

<h2>@ViewData["Title"]</h2>>

<table> 
    <tr>
        <th>Username</th>
        <th>E-mail</th>
        <th></th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <th>@user.Username</th>
            <th>@user.Email</th>
            <th></th>
        </tr> 
    }
</table>